html{
    height:100%;
    font-size: 6px
}
body{
    height: 100%;
}
div{
    font-size: 0.9375rem;
}
.page_container{
    height: 100%;
    width: 100%;
    display: flex;
    flex-direction: column;
}
.page_body{
    height: 100%;
    width: 100%;
    flex-grow: 1;
    display: flex;
    flex-direction: column;
    align-items: center;
    position: relative;
}
.footer{
    flex-grow: 1;
}
.login_section{
    display: flex;
    padding: 2.8125rem;
    align-self: start;

}
.login_section_icon{
    background-image: url(./user_icon.png);
    background-size: 90%;
    background-repeat: no-repeat;
    background-position:center center ;
    height: 5.625rem;
    width: 5.625rem;
}
.login_section_content{
    padding: 0.9375rem;
    display: flex;
    flex-direction: column;
}
.login_section_click{
    font-size: 1.25rem;
    font-weight: bold;


}
#wode6_login_section_click{
    display: flex;
}
.clickContent {
    color:rgb(123, 123, 123)
}
.serviceDesc{
    color:rgb(123, 123, 123)
}
.partion{
    height: 1.5625rem;
    width: 100%;
    background-color: rgb(237, 237, 237);
}
.moreService{
    width: 100%;
    display: flex;
    justify-content: space-evenly;
    padding: 3.125rem 0 2.1875rem 0;
}
.moreService div:nth-child(1) .serviceIcon{
    background-image: url(./history_icon.png);

}
.moreService div:nth-child(2) .serviceIcon{
    background-image: url(./feedback_icon.png);

}
.moreService div:nth-child(3) .serviceIcon{
    background-image: url(./afterService_icon.png);

}
.moreService div:nth-child(4) .serviceIcon{
    background-image: url(./vip_icon.png);

}
.serviceIcon_wode6{
    background-image: url(./vip_icon.png);
    background-size: 90%;
    width:3.125rem;
    height: 3.125rem;
    background-position: center center;
    background-repeat: no-repeat;
}
.serviceIcon{
    background-size: 90%;
    width:3.125rem;
    height: 3.125rem;
    background-position: center center;
    background-repeat: no-repeat;
}
.buyPrivilege{
    background-image: url(../../image/privilegeCard_icon.png);
    background-repeat: no-repeat;
    background-position: center center;
    background-size: 100% 100%;
    width: 98%;
    height: 5rem;
}
.login_section_icon_wode5{
    background-image: url(./vip_icon.png);
    background-size: 90%;
    background-repeat: no-repeat;
    background-position:center center ;
    height: 5.625rem;
    width: 5.625rem;
}
.login_section_left div:nth-child(2){
    color: rgb(123, 123, 123);
    font-size: 1.25rem;
}
.wode5_login_section_left{
    width: 100%;

}
#wode5_point1{
font-size: 1.875rem;
font-weight: lighter;
}
#wode5_point1>a{
    font-size: 1.5rem;
    font-weight: lighter;
}
#wode5_login_section{
    width: 80%;
    justify-content: space-around;

}
#wode5_login_section_content{
    line-height: 5.3125rem;
}
#alter_icon{
    margin:0.15625rem 0 0 0.9375rem ;
    background-image: url(./alter_icon.png);
    background-size: 90% 90%;
    background-position: center center;
    width: 1.25rem;
    height: 1.25rem;
    background-repeat: no-repeat;
}
.service_row_vertical{
    display: flex;
    justify-content: center;
    flex-wrap: wrap;
}
.row_vertical_item{
    display: flex;
    padding: 3.125rem;
    align-items: center;
    color: rgb(123, 123, 123);

}
.row_vertical_content{
    font-size: 1.09375rem;
    margin-left: 0.46875rem;
}
.service_row_vertical .row_vertical_item:nth-child(1) .row_vertical_icon{
    background-image: url(./gou_icon.png);
}
.service_row_vertical .row_vertical_item:nth-child(2) .row_vertical_icon{
    background-image: url(./invite_icon.png);
}
.service_row_vertical .row_vertical_item:nth-child(3) .row_vertical_icon{
    background-image: url(./note_icon.png);
}
.service_row_vertical .row_vertical_item:nth-child(4) .row_vertical_icon{
    background-image: url(./afterService_icon.png);
}
.row_vertical_icon{
    background-size: 90% 90%;
    background-position: center center;
    width: 2.8125rem;
    height: 2.8125rem;
    background-repeat: no-repeat;
}
#wode6_userContent{
    border-radius: 0.3125rem;
    border: 1px solid rgb(221, 148, 119);
    display: flex;
}
#deadline{
margin-left: 0.3125rem;
}
.inputEntity{
    margin-left:0.625rem ;
    height: 2.8125rem;
    font-size: 1.25rem;
    outline: none;
    border: none;
    border-bottom: 2px solid rgb(123, 123, 123);
    background-color: rgb(253,253,253);
}
.input_item{
    display: flex;

}
.input_label{
line-height: 2.8125rem;
text-align: center;
font-size: 1.25rem;
color: rgb(123, 123, 123);
}
.wode7_inputs{
    height: 50%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-evenly;
}
#iconEntity{
    line-height: 2.8125rem;
    margin-left:0.625rem ;
    width: 15.21875rem;
    height: 2.8125rem;
    font-size: 1.25rem;
    color: rgb(123, 123, 123);
    border-bottom: 2px solid rgb(123, 123, 123);
}
#xufei{
    margin-left: 50px;
}
#wode8_inputEntity{
    border:none;
}
#wode8_input_item{
    padding: 6.25rem 0 0 0;
    border-bottom: 2px solid rgb(123, 123, 123);
    margin-bottom: 4.6875rem;
}
