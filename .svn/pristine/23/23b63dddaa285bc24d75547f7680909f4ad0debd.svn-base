html{
    height: 100%;
    font-size: 6px
}
*{
    margin: 0;
}
body{
    height:100%;
    width: 100%;
    box-sizing: border-box;
}
.login_container{
    height: 100%;
    display: flex;

    flex-direction: column;
    justify-content: center;
    align-items: center;
}
.surface_container{
    background-image: url(./jzyd_icon.png);
    height: 70%;
    width: 100%;
    background-size: 100% 100%;
    background-position: 5rem -2.5rem;
    background-repeat: no-repeat;
}
.privilegeContent{
    display: flex;
    flex-direction: column;
    align-items: flex-end;
    color: rgb(193, 24, 33);
    font-size: 2.5rem;
}
.login_button{
    height: 5.625rem;
    width: 80%;
    border-radius: 1.25rem;
    font-size: 2.5rem;
    line-height: 5.625rem;
    text-align: center;
    margin: 1.25rem;
}
.content_itme{

}
.button1{
    background-color: rgb(193, 24, 33);
    color: #fff;
}
.button2{
    background-color: #fff;
    color: #000;
    border: 1px solid #000;
}
.surface_container2{
    background-image: url(./jzyd_icon2.png);
    height: 40rem;
    width: 100%;
    background-size: 100% 100%;
    background-position: center center;
    background-repeat: no-repeat;
}
#login_container_login2{
    justify-content: flex-start;
    height: 90%;
}
.partion{
    height: 3.125rem;
    width: 100%;
    background-color: rgb(237, 237, 237);
}
#partion_login2{
    line-height: 3.125rem;
    text-align: center;
    font-size: 1.875rem;
}
#login2_button1{
    position: relative;
    bottom: -37.5rem
}
#login2_button2{
    position: relative;
    bottom: -37.5rem;
}
