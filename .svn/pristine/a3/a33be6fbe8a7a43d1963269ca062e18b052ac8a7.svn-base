    <script type="text/javascript">
        apiready = function(){
    			var aLi = $api.domAll('#title-box-ul li');
    			window.mySwipe = Swipe($api.byId('slider'),{
    					auto: 3000,
    					continuous: true,
    					callback: function(index,elem) {
                  // alert(index);
                  if (index > 2) { return }
    							var i = aLi.length;
    							while (i--) {
                    aLi[i].className = '';
    							}
                  if (index <= 2) {
                    aLi[ index ].className = 'active';
                  }
    					}
    			});
        }

        // 用于链接到淘宝链接
        var toDetail = function(obj){
            var url = $api.attr(obj,'data-url');
            var title = $api.attr(obj,'data-title');

            api.execScript({
                name: 'root',
                script: 'indexToDetail("'+title+'","'+url+'")'
            });
        };
