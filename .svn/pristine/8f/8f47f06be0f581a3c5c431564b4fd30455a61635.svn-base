html{
    height:100%;
}
body{
    height:100%;
}
.page_container{
    display: flex;
    flex-direction: column;
    width: 100%;

    /* align-items: center; */
    height: 100%;
}
.login_body{
    height: 100%;
    flex-grow: 1;
    display: flex;
    flex-direction: column;
    /* justify-content: space-around; */
    align-items: center;
    position: relative;
}
.footer{
    flex-grow: 1;
}
.login_title{
    font-size: 75px;

    padding: 80px 60px;
}
#getCode{
    text-decoration: none;
    color: #fff;
    font-size: 40px;
}
.login_input{
    display: flex;
    font-size: 40px;
    margin: 20px;
    border-bottom:2px solid rgb(143, 143, 143) ;

}
.login_getCode{
    width: 80%;
    height: 70px;
    border-radius: 30px;
    background-color: rgb(193, 24, 33);
    text-align: center;
    line-height: 70px;
    margin: 60px;
    margin-bottom: 0;

}
.login_regist{
  width: 80%;
  height: 70px;
  border-radius: 30px;
  background-color: rgb(253,253,253);
  text-align: center;
  line-height: 70px;
  margin: 60px;
  border: 2px solid rgb(193, 24, 33);
}
#regist{
    text-decoration: none;
    color:  rgb(193, 24, 33);
    font-size: 40px;
}
#input_telFore{
    font-size: 40px;
    border: none;
    margin-right:10px ;
    background-color: rgb(253,253,253);
}
#input_element{
    height: 90px;
    font-size: 40px;
    outline: none;
    border: none;
    background-color: rgb(253,253,253);

}
.login_codeInput{
    display: flex;
    justify-content: center;
    align-items: center;
}
.codeSquare{

    margin:16px;
}
.input_element2{
    width: 80px;
    height: 80px;
    font-size: 40px;
    text-align: center;
}
.tip{
    font-size: 16px;
    font-size: 35px;
}
.login_codeTip1{
    margin: 80px;
}
.login_codeTip2{
    color:rgb(143, 143, 143);
    position: absolute;
    bottom: 20px;
}
.login_otherWays{
    display: flex;
    width: 80%;
    justify-content: space-around;
    position: absolute;
    bottom: 120px;
}
.login_otherWays div:nth-child(1){
    background-image: url(./weixin.png);

}
.login_otherWays div:nth-child(2){
    background-image: url(./qq.png) ;

}
.login_otherWays div:nth-child(3){
    background-image: url(./weibo.png);

}
.login_otherWays div{
    background-repeat: no-repeat;
    background-position: center center;
    background-size: 100%;
}
.other_image{
width: 150px;
height: 150px;

}
#code_tip_contract{
    color:rgb(143, 143, 143)
}
#wode3_input{
    width: 70%;
}
#wode3_input div{
    margin-top: 60px;
}
.partion{
    height: 50px;
    width: 100%;
    background-color: rgb(237, 237, 237);
}
#wode9_tip{
    color: rgb(143, 143, 143);
    width: 487px;
    height: 90px;
    align-self: center;
    margin-top: 100px;
}
.idcard_square{
    display: flex;
    position: relative;
    left: 40px;
}
.square_image{
    margin:30px 30px 60px 30px;
    font-size: 150px    ;
    line-height: 230px;
    text-align: center;
    width: 250px;
    height: 250px;
    border: 2px solid rgb(143, 143, 143);
    color:rgb(143, 143, 143);
}
.attentions{
    margin-top: 80px;
    color: rgb(143, 143, 143);
    width: 80%;
    position: relative;
    left: 100px;

}
.attention {
    width: 80%;
    display: flex;
    justify-content: space-between;
    font-size: 40px;
}
 .attentions .attention:nth-child(1){
     justify-content: flex-start;
 }
 #wode10_partion{
        margin-top: 70px;

 }
 .wode11_tip{
     display: flex;
     font-size: 40px;
     margin-top: 60px;
     color: rgb(143, 143, 143);

 }
 #tixian_amount{
     margin-left: 20px;
 }
