*{
    margin: 0;
}
.bofang_nav_flex{
    display: flex;
    padding-top: 50px;
    justify-content: space-between;
    background-color: rgb(253, 253, 253);
    flex-grow: 1;
    border-bottom: 4px solid rgb(219, 219, 220);
    height: 120px;
}
.nav_left{
    display: flex;
    align-items: center;
}
.nav_right{
    display: flex;
    align-items: center;
}

.backButton{
    font-size: 70px;
    font-family: Georgia;
    width: 70px;
    color: #000;
}
.backContent{
    font-size: 38px;
    color: #666;

}
.nav_left div{
    text-align: center;
    line-height: 70px;
    height: 70px;
}
.imageContainer{

    width: 50px;
    height: 50px;
    margin:15px;
    background-repeat: no-repeat;
    background-position:center;
    background-size: 90% 90%;

}
.image_item{
    font-size: 23px;
    display: flex;
    flex-direction: column;
    align-items: center;
    width: 100px;
}
.nav_right div:nth-child(1) .imageContainer{
    background-image: url(./dy_icon.png);

}
.nav_right div:nth-child(2) .imageContainer{
    background-image: url(./feedback_icon.png);

}
.nav_right div:nth-child(3) .imageContainer{
    background-image: url(./hot_icon.png);

}
.nav_right div:nth-child(4) .imageContainer{
    background-image: url(./more_icon.png);

}
.nav_right div:nth-child(5) .imageContainer{
    background-image: url(./fenxiang_icon.png);
    margin-right: 15px;

}
