    apiready = function () {
        $api.fixStatusBar( $api.dom('.titlebar.activebar .topbar') );
        firstHeader = $api.byId('firstHeader');
        secHeader = $api.byId('secHeader');
        thirdHeader = $api.byId('thridHeader');
